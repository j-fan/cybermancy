    "build": "cross-env NODE_ENV=production webpack --color --progress",
    "postbuild": "ncp ./public ./docs --filter '**/*.*'",
    "start": "serve -p 5011 docs",
    "lint": "eslint --ext .tsx,.ts,.js --quiet --fix src"
  },
