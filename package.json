    "eslint-plugin-react": "^7.22.0",
    "ncp": "^2.0.0",
    "react": "17.0.0",
    "react-babylonjs": "3.0.8",
    "react-dom": "17.0.0",
    "react-use-measure": "^2.0.3"
  },
